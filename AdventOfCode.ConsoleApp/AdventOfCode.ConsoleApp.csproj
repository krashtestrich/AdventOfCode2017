<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>AdventOfCode.ConsoleApp</AssemblyName>
    <RootNamespace>AdventOfCode.ConsoleApp</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\AdventOfCode.Logic\AdventOfCode.Logic.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Inputs.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Inputs.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Inputs.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Inputs.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
